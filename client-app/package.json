{
  "name": "coinpush",
  "displayName": "App",
  "version": "0.0.1",
  "description": "CoinPush app",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/DutchKevv/TradeJS.git"
  },
  "main": "index.js",
  "scripts": {
    "build-mobile": "npm run copy-www && cordova build --no-inc",
    "build-android": "npm run copy-www && cordova build android --no-inc",
    "build-ios": "npm run copy-www && cordova build ios --no-inc",
    "build-mobile-release": "npm run copy-www && cordova build --release --inc-version",
    "build-android-release": "npm run copy-www && cordova build android --release --inc-version",
    "build-ios-release": "npm run copy-www && npm run clean && cordova build ios --release --inc-version",
    "emulate-android": "npm run copy-www && cordova emulate android",
    "emulate-ios": "npm run copy-www && cordova emulate ios",
    "run-android": "npm run copy-www && cordova run android",
    "run-ios": "npm run copy-www && cordova run ios",
    "clean": "",
    "clean2": "cordova clean",
    "copy-www": "ncp ./../client/www ./www "
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova": "^8.0.0",
    "cordova-android": "^7.1.0",
    "cordova-browser": "5.0.3",
    "cordova-plugin-admobpro": "^2.31.4",
    "cordova-plugin-badge": "^0.8.7",
    "cordova-plugin-extension": "^1.5.4",
    "cordova-plugin-facebook4": "^1.10.1",
    "cordova-plugin-fileopener": "^1.0.5",
    "cordova-plugin-inappbrowser": "^2.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-plugin-x-toast": "^2.6.2",
    "cordova-support-google-services": "^1.1.0",
    "ncp": "^2.0.0",
    "npm": "^5.8.0",
    "phonegap-facebook-plugin": "^0.12.0",
    "phonegap-plugin-push": "^2.2.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-admobpro": {},
      "cordova-plugin-fileopener": {},
      "cordova-plugin-badge": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-facebook4": {
        "APP_ID": "178901869390909",
        "APP_NAME": "“CoinPush”"
      },
      "cordova-support-google-services": {},
      "phonegap-plugin-push": {
        "ANDROID_SUPPORT_V13_VERSION": "27.+",
        "FCM_VERSION": "11.6.2"
      },
      "cordova-plugin-statusbar": {},
      "cordova-plugin-x-toast": {}
    },
    "platforms": [
      "browser",
      "android"
    ]
  }
}