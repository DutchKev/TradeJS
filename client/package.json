{
  "name": "coinpush",
  "displayName": "App",
  "version": "0.0.24",
  "description": "CoinPush client",
  "repository": {
    "type": "git",
    "url": "https://github.com/DutchKevv/CoinPush.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "ng build --watch --aot --output-hashing=none",
    "build-prod": "ng build --build-optimizer --prod --aot --no-progress --output-hashing=none --configuration=prod",
    "build-prod-stats": "ng build --build-optimizer --prod --aot --stats-json --output-hashing=none",
    "build-prod-verbose": "ng build --build-optimizer --prod --stats-json --output-hashing=none --verbose > build.txt",
    "load-symbol-images": "node ./tools/load-symbol-images.js",
    "build-spritesheet": "node ./tools/build-spritesheet.js",
    "bundle-report": "webpack-bundle-analyzer ../static/clients/web/stats.json",
    "install-shared": "cd ../shared/modules/coinpush && npm i",
    "coinpush": "file:/usr/src/app/shared/modules/coinpush"
  },
  "author": "HK Brandsma",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular-devkit/build-angular": "latest",
    "@angular/cli": "^6.0.3",
    "@angular/common": "6.0.2",
    "@angular/compiler": "6.0.2",
    "@angular/compiler-cli": "6.0.2",
    "@angular/core": "6.0.2",
    "@angular/forms": "6.0.2",
    "@angular/platform-browser": "6.0.2",
    "@angular/router": "6.0.2",
    "@ng-bootstrap/ng-bootstrap": "2.0.0",
    "bootstrap": "4.1.1",
    "coinpush": "file:../shared/modules/coinpush",
    "deepmerge": "2.1.0",
    "highcharts": "6.1.0",
    "javascript-time-ago": "^1.0.30",
    "localforage": "1.7.1",
    "rxjs": "6.1.0",
    "socket.io-client": "2.1.1",
    "typescript": "2.7.2",
    "webpack-bundle-analyzer": "2.13.0",
    "zone.js": "0.8.26"
  }
}