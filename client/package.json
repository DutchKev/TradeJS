{
  "name": "coinpush",
  "displayName": "App",
  "version": "1.0.0",
  "description": "CoinPush client",
  "repository": {
    "type": "git",
    "url": "https://github.com/DutchKevv/TradeJS.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "ng build --watch --aot ---env=development -output-hashing none",
    "build": "ng build --prod --aot --env=production --output-hashing none",
    "build-stats": "ng build --prod --aot --env=production --stats-json --output-hashing none",
    "build-verbose": "ng build --prod --env=production --stats-json --output-hashing none --verbose > build.txt",
    "load-symbol-images": "node ./tools/load-symbol-images.js",
    "build-spritesheet": "node ./tools/build-spritesheet.js",
    "bundle-report": "webpack-bundle-analyzer www/stats.json",
    "install-shared": "cd ../shared/modules/coinpush && npm i"
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@angular/cli": "1.7.4",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/compiler-cli": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/router": "5.2.10",
    "@ng-bootstrap/ng-bootstrap": "1.1.2",
    "bootstrap": "4.1.0",
    "highcharts": "6.1.0",
    "moment": "latest",
    "node-sass": "beta",
    "localforage": "1.7.1",
    "socket.io-client": "2.1.0",
    "typescript": "2.8",
    "webpack-bundle-analyzer": "2.11.1",
    "zone.js": "0.8.26",
    "coinpush": "file:/usr/src/app/shared/modules/coinpush"
  }
}
