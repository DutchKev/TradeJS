{
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": false,
    "lib": ["dom", "es2017"],
    "target": "es5",
    "paths": {
      "coinpush/*": [
        "../../shared/modules/coinpush/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "genDir": "./aot",
    "comments": false,
    "entryModule": "./app/app.module#AppModule",
    "skipMetadataEmit": true
  },
  "exclude": [
    "node_modules",
    ".npm"
  ]
}