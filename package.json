{
  "releaseName": "coinpush",
  "name": "coinpush",
  "version": "0.0.1-alpha-4",
  "email": "kewin@frontend-freelance.com",
  "author": "Kewin Brandsma",
  "main": "index.js",
  "private": false,
  "license": "CC-BY-NC-ND-4.0",
  "description": "Javascript Trading Client and Server",
  "repository": {
    "type": "git",
    "url": "https://github.com/DutchKevv/CoinPush.git"
  },
  "keywords": [
    "TradeJS",
    "Stock",
    "Forex",
    "Trade",
    "Trading",
    "Node",
    "Trading",
    "NodeJS"
  ],
  "scripts": {
    "copy-ssl": "cp -r /etc/letsencrypt/ ./server-nginx/ssl && chmod -R 777 ./server-nginx/ssl/",
    "build-all": "docker-compose build",
    "build-client": "docker-compose build client",
    "build-client-app": "docker-compose build client-app",
    "build-jenkins": "docker-compose build jenkins",
    "build-server": "docker-compose build mongodb redis gateway notify user event cache comment",
    "build-nginx": "docker-compose build nginx",
    "build-prod-all": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build",
    "build-prod-client": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build client",
    "build-prod-jenkins": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build jenkins",
    "build-prod-server": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build redis gateway notify user event cache comment",
    "build-prod-nginx": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml build nginx",
    "start": "npm run d-up",
    "start-server": "docker-compose up -d mongodb redis gateway notify cache event comment user",
    "start-client": "docker-compose up client",
    "start-jenkins": "docker-compose up jenkins",
    "start-nginx": "docker-compose up --build nginx",
    "restart-server": "npm run stop-server && npm run start-server",
    "prod-client": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up client && npm run inc-build-number",
    "prod-jenkins": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d jenkins",
    "prod-server": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d redis gateway notify user event cache comment",
    "prod-nginx": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up --build -d nginx",
    "stop-server": "docker-compose stop redis gateway notify user event cache comment mongodb",
    "d-rmi-none": "docker rmi $(docker images --filter 'dangling=true' -q --no-trunc)",
    "d-rmi-all": "docker rmi -f $(docker images -q)",
    "d-rmi-dangling": "docker rmi $(sudo docker images --filter \"dangling=true\" -q --no-trunc)",
    "d-rm-all": "docker-compose rm -f",
    "d-mongo-shell": "docker exec -it mongodb mongo",
    "d-stop": "docker-compose stop",
    "d-build": "docker-compose build",
    "d-up": "docker-compose up",
    "d-prune": "docker system prune",
    "clean-client": "cd ./scripts && sh ./clean-npm-modules.sh",
    "clean-app": "cd ./scripts && sh ./clean-npm-modules.sh",
    "clean-server": "cd ./scripts && sh ./clean-npm-modules.sh",
    "clean-db": "",
    "clean-candles": "docker exec -it mongodb bash -c 'mongo < myFile.js'",
    "_prod": "pm2 start ecosystem.config.js --env production && pm2 monit",
    "inc-build-number": "node ./scripts/inc-build-number.js",
    "test": "echo $(pwd)",
    "get-ssl-cert": "docker run -v $(pwd)/server-nginx/ssl:/etc/letsencrypt -e http_proxy=$http_proxy -e domains=\"coinpush.app,jenkins.coinpush.app\" -e email=\"kewin@frontend-freelance.com\" -p 80:80 -p 443:443  --rm pierreprinetti/certbot:latest --expand"
  },
  "dependencies": {}
}
